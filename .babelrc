{
  "presets": [
    [
      "env", {
          "targets": {
              "browsers": ["last 2 Chrome versions"]
          },
          "modules": false,
          "useBuiltIns": true
      }
    ],"react"],
  "plugins": ["syntax-dynamic-import"],
  "env": {
    "development": {
        "plugins": [
            ["react-transform", {
               "transforms": [{
                 "transform": "react-transform-hmr",

                 "imports": ["react"],

                 "locals": ["module"]
               }]
            }]
       ]
    }
  }
}
